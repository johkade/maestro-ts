  "devDependencies": {
    "@types/node": "^18.14.0",
    "typescript": "^4.9.5"
  },
  "typings": "./maestro.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johkade/maestro-ts.git"
  },
